extends base.pug

block scripts
    script(src="../static/index.js") 

block style
    style 
        include ../static/style.css
        include ../static/styleadmission.css

block content
    div.container
        h1 CONNECT WITH US
        form(action="/admission",method="post", class="myform")
            input(type="text",class="myinput", name="name", placeholder="Name") 
            input(type="phone",class="myinput", name="phone", placeholder="Phone No.") 
            input(type="email",class="myinput", name="email", placeholder="E-mail ID") 
            input(type="address",class="myinput", name="address", placeholder="Address") 
            input(type="text",class="myinput", name="danceform", placeholder="Enter dance form") 
            button(class="btn")
                |Submit